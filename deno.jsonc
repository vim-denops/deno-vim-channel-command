{
  "lock": false,
  "tasks": {
    "check": "deno check **/*.ts",
    "test": "deno test -A --parallel --shuffle --doc",
    "test:coverage": "deno task test --coverage=.coverage",
    "coverage": "deno coverage .coverage",
    "upgrade": "deno run -A https://deno.land/x/udd/main.ts **/*.ts"
  }
}
